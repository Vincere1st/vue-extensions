{
  "name": "vue3-extensions",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Ostyn Vincent",
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build --target lib --name vue-extensions index.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Vincere1st/vue-extensions"
  },
  "bugs": {
    "url": "https://github.com/Vincere1st/vue-extensions/issues"
  },
  "main": "./dist/vue-extensions.common.js",
  "dependencies": {
    "core-js": "^3.21.1",
    "npm": "^8.5.3",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "vite": "^2.7.13",
    "vue-template-compiler": "^2.6.10",
    "vue-tsc": "^0.31.1",
    "typescript": "~4.5.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ]
  }
}
